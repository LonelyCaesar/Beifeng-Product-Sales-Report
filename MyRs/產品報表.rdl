  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@typeID">
            <Value>=Parameters!typeID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT A.產品編號, A.產品,C.供應商,B.類別名稱,A.單位數量,A.單價,A.庫存量,A.產品圖片
FROM 產品資料 AS A JOIN 產品類別 AS B ON A.類別編號=B.類別編號
    JOIN 供應商 AS C ON A.供應商編號=C.供應商編號
WHERE A.類別編號 IN(@typeID)</CommandText>
      </Query>
