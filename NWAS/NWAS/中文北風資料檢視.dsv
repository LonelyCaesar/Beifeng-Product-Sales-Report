    <xs:schema id="中文北風資料檢視" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" xmlns:msprop="urn:schemas-microsoft-com:xml-msprop">
      <xs:element name="中文北風資料檢視" msdata:IsDataSet="true" msdata:UseCurrentLocale="true" msprop:design-time-name="8ef4c4da-f329-41b7-aafc-0026b57892c9">
        <xs:complexType>
          <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element name="dbo_訂貨主檔" msprop:design-time-name="bd086f61-3e98-4dd5-8f08-94ad512f9521" msprop:IsLogical="True" msprop:FriendlyName="訂貨主檔" msprop:DbTableName="訂貨主檔" msprop:TableType="View" msprop:QueryDefinition="SELECT  A.訂單號碼, A.訂單日期, A.客戶編號, A.員工編號, A.運費, A.送貨城市, B.貨運公司名稱 AS 運送方式&#xD;&#xA;FROM      訂貨主檔 AS A INNER JOIN&#xD;&#xA;                   貨運公司 AS B ON A.送貨方式 = B.貨運公司編號" msprop:QueryBuilder="SpecificQueryBuilder">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="訂單號碼" msprop:design-time-name="7822f26f-c2b9-4f6c-bd38-07a8d5379dc7" msprop:DbColumnName="訂單號碼" msprop:FriendlyName="訂單號碼" type="xs:int" />
                  <xs:element name="客戶編號" msprop:design-time-name="49494a07-3023-4a21-ab79-d0cc775a32e4" msprop:DbColumnName="客戶編號" msprop:FriendlyName="客戶編號" minOccurs="0">
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:maxLength value="5" />
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element name="員工編號" msprop:design-time-name="7862b221-2988-41f9-895f-d5b973eb6be4" msprop:DbColumnName="員工編號" msprop:FriendlyName="員工編號" type="xs:int" minOccurs="0" />
                  <xs:element name="訂單日期" msprop:design-time-name="57bd4860-5907-4e0c-9508-9e918d4f029b" msprop:DbColumnName="訂單日期" msprop:FriendlyName="訂單日期" type="xs:dateTime" minOccurs="0" />
                  <xs:element name="運費" msprop:design-time-name="d8a6b87e-c8e4-4917-9b28-fb84c78c98d4" msprop:DbColumnName="運費" msprop:FriendlyName="運費" type="xs:decimal" minOccurs="0" />
                  <xs:element name="送貨城市" msprop:design-time-name="58f7e0b6-a86d-4c1b-a74c-bb99928e8aff" msprop:DbColumnName="送貨城市" msprop:FriendlyName="送貨城市" minOccurs="0">
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:maxLength value="15" />
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                  <xs:element name="運送方式" msprop:design-time-name="4e725797-e04a-40c3-bcb1-58f0977fa5bc" msprop:DbColumnName="運送方式" minOccurs="0">
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:maxLength value="40" />
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:element>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
            <xs:element name="dbo_訂貨明細" msprop:design-time-name="27bdc969-c4ec-4378-bbb8-22f81e61de33" msprop:IsLogical="True" msprop:FriendlyName="訂貨明細" msprop:DbTableName="訂貨明細" msprop:TableType="View" msprop:QueryDefinition="SELECT  訂單號碼, 產品編號, 數量, ROUND((單價 * 數量) * (1 - 折扣), 0) AS 金額&#xD;&#xA;FROM      訂貨明細" msprop:QueryBuilder="SpecificQueryBuilder">
              <xs:complexType>
                <xs:sequence>
